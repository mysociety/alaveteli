<% case tagging %>
<% when PublicBody %>
  <%= render partial: 'admin_public_body/public_body', locals: {
    public_body: tagging
  } %>

<% when InfoRequest %>
  <%= render partial: 'admin_request/info_request', locals: {
    info_request: tagging
  } %>

<% when OutgoingMessage::Snippet %>
  <%= render partial: 'admin/outgoing_messages/snippets/snippet', locals: {
    snippet: tagging
  } %>

<% when Note %>
  <%= render partial: 'admin/notes/note', locals: {
    note: tagging
  } %>

<% when Blog::Post %>
  <%= render partial: 'admin/blog_posts/blog_post', locals: {
    blog_post: tagging
  } %>

<% when User %>
  <%= render partial: 'admin_user/user', locals: {
    user: tagging
  } %>

<% else %>
  <pre><%= tagging.to_json %></pre>
<% end %>
